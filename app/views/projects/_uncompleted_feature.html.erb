<li class="lg-item" id="uncompleted-feat-<%= feat.id %>">
  <%= feat.name %>

  <br /><br />

  <div class="features-username">
    <p><%= feat.owner_name %></p>
    <p><%= feat.created_at.strftime("%-d/%-m/%Y") %></p>
  </div>

  <% if feat.is_owner_or_not(user) %>
    <div class="close-feature-form-div" id="close-feature-form-<%= feat.id %>">
      <%= render 'projects/close_feature', p: proj, f: feat %>
    </div>
  <% end %>
</li>
